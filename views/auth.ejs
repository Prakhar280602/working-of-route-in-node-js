<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h1>Hello for auth route</h1>

    <h1>
        <%= info %>
            <br>
              <!--  //for object we use dot -->
              <%= profile.naam%>

            <!-- //for array treversing -->
            <ol>
                <% subjects.forEach(function(vishay) { %>
                    <li>
                        <%= vishay %>
                    </li>
                    <% }) %>
            </ol>
            <br>
            <hr>

           <% users.forEach(function(user){ %>
                
                  <h1><%= user.id %></h1> 
                 
                  <h1> <%= user.name %></h1>
                   
                  <h1> <%= user.email %></h1>

                  <a href="/read/<%=user.id%>"><button>Read</button></a>
                  <a href="/update/<%=user.id%>"><button>Update</button></a>
                 <a href="/delete/<%=user.id%>"><button>Delete</button></a>
                <hr>
               
            <% }) %>


          
    </h1>
    <a href="/"><button>Back to Home page</button></a>

</body>

</html>